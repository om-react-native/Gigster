{
  "expo": {
    "name": "Gigster",
    "slug": "gigster",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splashScreen.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFFFFF"
    },
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.gigster.app",
      "buildNumber": "1",
      "infoPlist": {
        "CFBundleDisplayName": "Gigster",
        "NSPhotoLibraryUsageDescription": "Gigster needs access to your photo library to set your profile picture and upload video resumes.",
        "NSPhotoLibraryAddUsageDescription": "Gigster needs permission to save photos to your library."
      }
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
